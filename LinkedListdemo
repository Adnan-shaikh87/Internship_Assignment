import java.util.*;
public class LinkedListExample {
    public static void main(String[] args) {
        List<Integer> numbers = new LinkedList<>();
        numbers.add(10);
        numbers.add(20);
        numbers.add(30);
        numbers.add(10); 

        System.out.println("Numbers List: " + numbers);
        System.out.println("First Number: " + numbers.get(2));
    }
}
